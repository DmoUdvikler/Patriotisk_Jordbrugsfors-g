@model IEnumerable<Patriotisk.Models.Experiment>

@{
    ViewBag.Title = "Experiments";
    Layout = "~/Views/Shared/PatriotiskLayout.cshtml";
}

<h2>Forsøg</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Treatment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Crop)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FieldName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Year)
        </th>

        <th></th>
    </tr>

    @if (Model != null)
    {
        foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Treatment)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Crop)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FieldName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Year)
                </td>
                <!-- Not implemented
                <td>
                    @Html.ActionLink("Se Data", "ShowData", new { id = item.Id.ToString() })
                </td>
                        -->
            </tr>
        }
    }

</table>

